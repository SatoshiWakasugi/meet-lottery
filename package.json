{
  "name": "meet-picker",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint --ext .ts,tsx --ignore-path .gitignore ."
  },
  "husky": {
    "hooks": {
